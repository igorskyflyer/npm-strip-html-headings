{
    "name": "@igorskyflyer/strip-html-headings",
    "version": "1.1.0",
    "description": "🍛 Lightweight utility to strip HTML heading tags (h1-h6). Remove headings completely or extract their text content while cleaning up markup. Perfect for transforming HTML, parsing content, or preparing plain text output. 🍤",
    "sideEffects": false,
    "engines": {
        "node": ">=22.0"
    },
    "type": "module",
    "types": "./dist/index.d.ts",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "test": "vitest --run",
        "pub": "pnpm test && git push origin main --tags"
    },
    "keywords": [
        "clean",
        "content",
        "extract",
        "h1",
        "h2",
        "h3",
        "h4",
        "h5",
        "h6",
        "heading",
        "headings",
        "html",
        "igor dimitrijević",
        "igorskyflyer",
        "markup",
        "parser",
        "plain-text",
        "remove",
        "sanitize",
        "strip",
        "tags",
        "text",
        "utility"
    ],
    "license": "MIT",
    "author": "Igor Dimitrijević <support@igorskyflyer.me> (https://github.com/igorskyflyer)",
    "funding": "https://ko-fi.com/igorskyflyer",
    "homepage": "https://github.com/igorskyflyer/npm-strip-html-headings",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/igorskyflyer/npm-strip-html-headings.git"
    },
    "bugs": {
        "url": "https://github.com/igorskyflyer/npm-strip-html-headings/issues"
    },
    "dependencies": {
        "@igorskyflyer/strip-html": "^2.1.0"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.2.4",
        "@igorskyflyer/biome-config": "^2.1.0",
        "@types/node": "^22.18.5",
        "typescript": "^5.9.2",
        "vitest": "^3.2.4"
    },
    "publishConfig": {
        "access": "public"
    },
    "packageManager": "pnpm@10.17.0"
}
